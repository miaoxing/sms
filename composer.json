{
  "name": "miaoxing/sms",
  "type": "miaoxing-plugin",
  "description": "Miaoxing SMS Plugin",
  "keywords": [
    "miaoxing",
    "sms"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.3"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Sms\\": "src"
    },
    "classmap": [
      "src/Lib"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Demo\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 6.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^7.0.1"
  }
}
